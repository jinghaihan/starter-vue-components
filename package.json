{
  "name": "lib-name",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.30.1",
  "description": "Starter template for Vue Components.",
  "author": "jinghaihan",
  "license": "MIT",
  "homepage": "https://github.com/jinghaihan/lib-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jinghaihan/lib-name.git"
  },
  "bugs": {
    "url": "https://github.com/jinghaihan/lib-name/issues"
  },
  "keywords": [],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsdown",
    "deps": "taze major -I",
    "lint": "eslint",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "release": "bumpp",
    "bootstrap": "pnpm install",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "catalog:build",
    "tsdown": "catalog:build",
    "unplugin-icons": "catalog:build",
    "unplugin-lightningcss": "catalog:build",
    "vite": "catalog:build",
    "bumpp": "catalog:dev",
    "happy-dom": "catalog:dev",
    "pncat": "catalog:dev",
    "simple-git-hooks": "catalog:dev",
    "taze": "catalog:dev",
    "tsx": "catalog:dev",
    "typescript": "catalog:dev",
    "@octohash/eslint-config": "catalog:lint",
    "eslint": "catalog:lint",
    "eslint-plugin-format": "catalog:lint",
    "lint-staged": "catalog:lint",
    "vue-tsc": "catalog:lint",
    "@vue/test-utils": "catalog:test",
    "vitest": "catalog:test",
    "@types/node": "catalog:types"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
